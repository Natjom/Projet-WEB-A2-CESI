{
  "name": "superstage/project",
  "description": "Un projet web PHP",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "post-install-cmd": [
      "echo 'Composer installé avec succès!'"
    ],
    "post-update-cmd": [
      "echo 'Les dépendances ont été mises à jour!'"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
